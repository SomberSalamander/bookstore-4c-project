@model List<Book>

@{
    ViewData["Title"] = "Home Page";
}

<h1>Knihy</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Author</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Book book in Model)
        {
            <tr>
                <th>@book.BookId</th>
                <td>@book.Title</td>
                <td>@book.Author</td>
            </tr>
        }
    </tbody>
</table>